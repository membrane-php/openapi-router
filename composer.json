{
  "name": "membrane/openapi-router",
  "type": "library",
  "license": "MPL-2.0",
  "autoload": {
    "psr-4": {
      "Membrane\\OpenAPIRouter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Membrane\\OpenAPIRouter\\Tests\\": "tests/",
      "Membrane\\OpenAPIRouter\\Tests\\Fixtures\\": "tests/fixtures/"
    }
  },
  "require": {
    "php": "^8.1.0",
    "cebe/php-openapi": "^1.7",
    "membrane/openapi-reader": "^1.0.0",
    "psr/http-message": "^1.0 || ^2.0",
    "psr/log": "^3.0",
    "symfony/console": "^6.2"
  },
  "require-dev": {
    "infection/infection": "^0.27.0",
    "phpunit/phpunit": "^10.2",
    "phpstan/phpstan": "^1.8",
    "squizlabs/php_codesniffer": "^3.7",
    "guzzlehttp/psr7": "^2.4",
    "mikey179/vfsstream": "^1.6.7"
  },
  "bin": [
    "bin/membrane-router"
  ],
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
