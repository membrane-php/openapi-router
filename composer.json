{
  "name": "membrane/openapi-router",
  "type": "library",
  "license": "MPL-2.0",
  "autoload": {
    "psr-4": {
      "Membrane\\OpenAPIRouter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Membrane\\OpenAPIRouter\\Tests\\": "tests/",
      "Membrane\\OpenAPIRouter\\Tests\\Fixtures\\": "tests/fixtures/"
    }
  },
  "require": {
    "php": "^8.1.0",
    "devizzent/cebe-php-openapi": "dev-master@dev",
    "membrane/openapi-reader": "dev-add-3.0-responses@dev",
    "psr/http-message": "^1.0 || ^2.0",
    "psr/log": "^3.0",
    "symfony/console": "^6.0 || ^7.0"
  },
  "require-dev": {
    "infection/infection": "^0.27.0",
    "phpunit/phpunit": "^10.5.38",
    "phpstan/phpstan": "^1.12.7",
    "squizlabs/php_codesniffer": "^3.7",
    "guzzlehttp/psr7": "^2.4",
    "mikey179/vfsstream": "^1.6.7"
  },
  "bin": [
    "bin/membrane-router"
  ],
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
